using System;

public class KickstartRecurssion
{

  public Boolean PalindromwithSubString(String s)
  {

    if (s.Length == 0 || s.Length == 1)
    {
      return true;
    }

    if (s[0] != s[s.Length - 1])
    {
      return false;
    }

    return PalindromwithSubString(s.Substring(1, s.Length - 2));
  }



}
